

<h1>配送先登録/一覧</h1>
  <%= form_with model: @address,url:addresses_path,method: :post, local:true do |f| %>
   <h4>郵便番号（ハイフンなし）</h4>
    <%= f.text_field :postal_code %>
   <h4>住所</h4>
    <%= f.text_field :address %>
   <h4>宛名</h4>
    <%= f.text_field :name %>
    <%= f.submit '新規登録' %>
  <% end %>